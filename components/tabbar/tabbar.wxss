
.tabbar-wrap{
    position: fixed;
    bottom: 0;
    left:0;
    z-index: 1;
    width: 100%;
    height: 48px;
    border-top:1rpx solid #e5e5e5;
    display: flex;
    background-color: #ffffff;
}
.tabbar-wrap .tabbar-item{
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size:20rpx;
  color: #7F8389;
}
.tabbar-wrap .tabbar-item.active{
  color: #5CB3FF;
}
.tabbar-wrap .tabbar-item:nth-child(1) .img{
  width:55rpx;
}
.tabbar-wrap .tabbar-item:nth-child(2) .img{
  width:52rpx;
}
.border_right{
  position: relative;
}
.border_right::before {
    content: " ";
    position: absolute;
    right: 0;
    bottom: 0;
    top: 0;
    width: 1rpx;
    border-left: 1rpx solid #e5e5e5;
    color: #e5e5e5;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleX(0.5);
    transform: scaleX(0.5);
}