page{
  height: auto;
}
.edit-cardInfo-wrap {
    position: relative;
    z-index: 1;
    margin: 190rpx auto 40rpx;
}

.create_part_1 {
    margin: 0 30rpx;
    padding: 50rpx 40rpx 35rpx;
    height: 271rpx;
    background: rgba(0, 0, 0, 0.64);
    border-radius: 8rpx;
    box-sizing: border-box;
    display: flex;
    justify-content: flex-start;
    align-content: center;
}

.placeholder {
    color: #b2b2b2;
    font-size: 32rpx;
}

.company-logo {
    margin-right: 40rpx;
}

.company-logo .img-logo {
    width: 130rpx;
    height: 130rpx;
    border-radius: 50%;
}

.user-nick {
    margin-top: -10rpx;
    margin-bottom: 38rpx;
}

.user-job {
    margin-bottom: 12rpx;
}

.create_part_1  .input {
    font-size: 28rpx;
    color: rgba(255, 255, 255, 1);
    line-height: 48rpx;
}

.create_part_1 .placeholder {
    font-size: 28rpx;
    color: rgba(255, 255, 255, 0.8);
    line-height: 48rpx;
}

.create_part_2 {
    position: relative;
    background: #fff;
    margin-top: 30rpx;
    padding: 0 30rpx;
}

.section {
    position: relative;
    padding: 35rpx 0 35rpx 88rpx;
    border-bottom: 1px solid #ebeef5;
}

.create_part_2  input, .create_part_2 textarea {
    font-size: 32rpx;
    width:90%;
}

.btn-box .getLocationBtn {
    position: absolute;
    right: 0;
    top: 0rpx;
    left: auto;
    height: 130rpx; 
    width: 68rpx;
    z-index: 2;
}

.btn-box .getLocationBtn image {
    display: inline-block;
    height: 30rpx;
    width: 28rpx;
    margin-left: 40rpx;
    margin-top: 40rpx;
}

.add-margin {
    /* margin-bottom: 500rpx; */
    padding-bottom: 300rpx;
    /* background: red; */
}

.user-phone {
    background: url('data:image/png;base64,') no-repeat left center;
    background-size: 30rpx 34rpx;
}

.user-wx {
    background: url('data:image/png;base64,') no-repeat left center;
    background-size: 38rpx 32rpx;
}

.user-email {
    background: url('data:image/png;base64,') no-repeat left center;
    background-size: 33rpx 25rpx;
}

.user-addr {
    background: url('data:image/png;base64,') no-repeat left 40rpx;
    background-size: 34rpx 33rpx;
    border-bottom: none;
}

.btns {
    display: flex;
    justify-content: space-between;
    width: 440rpx;
    margin: 86rpx auto 0;
}

.btns .btn-box {
    position: relative;
    width: 200rpx;
    height: 80rpx;
}

.btns .btn-box .img {
    width: 100%;
}

.btns .btn-box .text {
    position: absolute;
    left: 0rpx;
    top: 0rpx;
    font-size: 34rpx;
    color: #fff;
    width: 200rpx;
    height: 80rpx;
    line-height: 80rpx;
    padding-left: 100rpx;
    box-sizing: border-box;
}

.btns .btn-box .text.and-text {
    line-height: 82rpx;
}

textarea {
    line-height: 1.4;
    margin: 0;
}

textarea.ios-textarea {
    margin-top: -15rpx;
    margin-left: -10rpx;
}

.btn-box image {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
}

button.form-input-btn {
    padding-left: 0;
    padding-right: 0;
    margin-left: 0;
    margin-right: 0;
    background: none;
    text-align: left;
    border: 0;
    border-radius: 0;
    overflow: visible;
}

button.form-input-btn::after {
    border: none;
}
.user-info-part{
    width:440rpx;
}
