.flow-btn{
  width:74rpx;
  height:232rpx;
  background:rgba(73,204,147,1);
  border-radius:32rpx 0px 0px 32rpx;
  color: #fff;
  position: fixed;
  right:0;
  top:50%;
  margin-top:-116rpx;
  font-size:28rpx;
  line-height:40rpx;
  padding:36rpx 20rpx 36rpx 26rpx;
  box-sizing: border-box;
}
.btn-area{
  display: flex;
  justify-content: space-between;
  width:640rpx;
  margin: 200rpx auto 0;
}
.btn-area.inside{
  justify-content: center;
}
.btn-area form{
  display: block;
}
.btn-area button{
  width:300rpx;
  height: 80rpx;
  border-radius: 40rpx;
  text-align: center;
  color: #fff;
  font-size: 34rpx;
  line-height: 76rpx;
}
.btn-area .btn-contact button{
  background:rgba(73,204,147,1);
  box-shadow:0rpx 2rpx 8rpx 0rpx rgba(139,223,186,1);
}
.btn-area .btn-share button{
  background:rgba(92,179,255,1);
  box-shadow:0rpx 2rpx 8rpx 0rpx rgba(92,179,255,1);
}
