@import "/template/share/share.wxss";

.page-wrap {
    min-height: 100%;
    background: #fff;
    position: relative;
}

.bg-color {
    height: 320rpx;
}

.user-card {
    min-height: 384rpx;
    background: rgba(255, 255, 255, 1);
    box-shadow: 0px 2px 10px 0px rgba(92, 179, 255, 0.4);
    border-radius: 8rpx;
    box-sizing: border-box;
    overflow: hidden;
    margin: 0 32rpx 40rpx;
    position: relative;
}

.card-info-1 {
    padding: 40rpx;
    position: relative;
}

.user-card .company-name {
    width: 430rpx;
    height: 120rpx;
    font-size: 40rpx;
    color: #333;
    margin-bottom: 36rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.user-card .user-nick, .user-card .user-job {
    font-size: 28rpx;
    color: #666;
    margin-bottom: 16rpx;
}

.user-card .user-job {
    margin-bottom: 0;
}

.user-card .setting {
    text-align: right;
    font-size: 28rpx;
    color: rgba(102, 102, 102, 1);
    margin-top: -10rpx;
}

.user-card .company-logo {
    position: absolute;
    top: 24rpx;
    right: 40rpx;
    width: 130rpx;
    height: 130rpx;
    border-radius: 50%;
    overflow: hidden;
}

.user-card .company-logo image {
    width: 100%;
    height: 100%;
    border-radius: 50%;
}
.user-card .no-info {
    width: 375rpx;
    height: 36rpx;
    background: rgba(242, 246, 252, 0.65);
}

.user-card .industry-scale.no-info {
    width: 227rpx;
}
.card-info-2 {
    border-top: 1px solid #e4e7ed;
    padding: 32rpx 40rpx 40rpx;
}

.card-info-2 view {
    padding-left: 48rpx;
    font-size: 28rpx;
    color: #333;
    line-height: 44rpx;
    margin-bottom: 25rpx;
}

.card-info-2 view:last-child {
    margin-bottom: 0;
}

.card-info-2 view.user-phone {
    background: url('data:image/png;base64,') no-repeat left 8rpx;
    background-size: 30rpx 34rpx;
}

.card-info-2 view.user-wx {
    background: url('data:image/png;base64,') no-repeat left 8rpx;
    background-size: 38rpx 32rpx;
}

.card-info-2 view.user-email {
    background: url('data:image/png;base64,') no-repeat left 8rpx;
    background-size: 33rpx 25rpx;
}

.card-info-2 view.user-addr {
    background: url('data:image/png;base64,') no-repeat left 8rpx;
    background-size: 34rpx 33rpx;
}

.cz-btns {
    padding: 120rpx 34rpx 20rpx;
    display: flex;
    justify-content: space-between;
    align-items: top;
}

.cz-btns>view {
    width: 50%;
}

.share-btn {
    width: 100rpx;
    background: none;
    color: #fff;
    font-size: 22rpx;
    text-align: left;
    margin-left: 0;
    padding:15rpx 0 0;
}

.lick-btn, .visit-btn {
    background: none;
    color: #fff;
    font-size: 26rpx;
     /* padding: 0;  */
    position: relative;
    display: block;
    text-align: left;
    padding-right:10rpx;
    margin-right:-10rpx;
}

.share-btn image {
    width: 32rpx;
    height: 32rpx;
    margin-right: 16rpx;
    vertical-align: middle;
}

.visit-btn image {
    width: 26rpx;
    height: 26rpx;
    margin-right: 16rpx;
    vertical-align: middle;
}

.right-btns {
    display: flex;
    justify-content: flex-start;
    align-items: top;
}

.visit-btn  .title {
    padding-left: 44rpx;
    line-height: 22rpx;
    font-size: 22rpx;
}

.lick-btn .title {
    padding-left: 52rpx;
    line-height: 22rpx;
    font-size: 22rpx;
}

.lick-btn image {
    width: 38rpx;
    height: 32rpx;
    margin-right: 16rpx;
    vertical-align: middle;
}

