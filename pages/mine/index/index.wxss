/* pages/mine/index/index.wxss */

.page-mine{
  min-height: 100%;
  background-color: #ffffff;
  padding:30rpx 30rpx 0;
  box-sizing: border-box;
  padding-top:64px;
}
.btn-contact{
  position: fixed;
  bottom:144rpx;
  right:0;
  z-index: 999;
  width:74rpx;
  height: 90rpx;
  border:none;
  background: transparent;
  padding:0;
}
.btn-contact::after{
  border:0;
}
.btn-contact .img{
  width:100%;
  height:100%;
}
.base-info{
  display: flex;
  margin-bottom: 32rpx;
  padding-top:20rpx;
}
.base-info .img-box{
  width:120rpx;
  height: 120rpx;
  border-radius: 50%;
  overflow: hidden;
  margin-right: 22rpx;
}
.base-info .img-box .img{
  width: 100%;
}
.base-info .info-content{
   flex:1;
   color: #B2B2B2;
}
.base-info .info-content .nick-member{
  display: flex;
  align-items: center;
  margin-bottom: 8rpx;

}
.base-info .info-content .nick{
  margin-right: 20rpx;
  max-width:345rpx;
}
.base-info .info-content .member{
  flex: 1;
}
.base-info .info-content .member .img{
  width:32rpx;
  height: 28rpx;
  margin-right: 8rpx;
  vertical-align: middle;
  position: relative;
  top:-2rpx;
}
.base-info .info-content .position{
  margin-bottom: 10rpx;
}
.base-info .info-content .company{
  margin-bottom: 22rpx;
}
.base-info .info-content .like-visitor{
   display: flex;
   font-size: 24rpx;
}
.base-info .info-content .like,.base-info .info-content .visitor{
  padding-left:26rpx;
}
.base-info .info-content .like-visitor text{
  margin-left:16rpx;
}
.base-info .info-content .like{
   margin-right:42rpx;
   background: url('data:image/png;base64,') left center no-repeat;
   background-size: 18rpx;
}
.base-info .info-content .visitor{
   background: url("data:image/png;base64,") left center no-repeat;
    background-size: 18rpx;
}
.reward-gold{
  width:692rpx;
  height:142rpx;
  padding:24rpx 34rpx 24rpx 30rpx;
  margin-bottom: 30rpx;
  box-sizing: border-box;
  background:rgba(255,255,255,1);
  box-shadow:0rpx 1rpx 8rpx 0rpx rgba(214,225,235,1);
  border-radius:16rpx;
  display: flex;
  align-items: center;
}
.reward-gold .img-box{
  width:66rpx;
  height:66rpx;
  margin-right: 20rpx;
}
.reward-gold .img-box .img{
  width:100%;
}
.reward-gold .gold-wrap{
  flex:1;
}
.reward-gold .gold-hd{
  color: #DBB272;
  margin-bottom: 10rpx;
}
.reward-gold .gold-bd{
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.nav-wrap{
  width:692rpx;
  padding:10rpx 30rpx 32rpx;
  margin-bottom: 24rpx;
  box-sizing: border-box;
  background:rgba(255,255,255,1);
  box-shadow:0rpx 2rpx 10rpx 0rpx rgba(211,211,211,0.68);
  border-radius:8rpx;
}
.nav-wrap .nav-item{
  display: flex;
  height: 96rpx;
  align-items: center;
}
.nav-wrap .nav-item .nav-item__hd{
  width:56rpx;
  height: 56rpx;
  margin-right: 32rpx;
}
.nav-wrap .nav-item .img{
  width:100%;
  height: 100%;
}
.nav-wrap .nav-item__bd{
  flex:1;
  display: flex;
  /* height: 48rpx;
  padding-top:25rpx;
  padding-bottom: 23rpx; */
  height: 96rpx;  
  align-items: center;
  justify-content: space-between;
}
.nav-wrap .nav-item__bd .nav-item__title{
  font-size: 33rpx;
  line-height: 48rpx;
  color: #333;
}
.btn-wrap button{
  width:670rpx;
  height: 112rpx;
}
